{
    "name": "conia/cms-boiler",
    "description": "Boiler templates for Conia CMS",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "ebene fÃ¼nf GmbH",
            "email": "conia@ebenefuenf.de",
            "role": "Developer"
        }
    ],
    "homepage": "https://conia.dev/cms-boiler",
    "keywords": [
        "conia",
        "cms",
        "templating",
        "templates",
        "boiler"
    ],
    "autoload": {
        "psr-4": {
            "Conia\\Cms\\Boiler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Conia\\Cms\\Boiler\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.1",
        "conia/boiler": "dev-main"
    },
    "require-dev": {
        "conia/cms": "@dev",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "nyholm/psr7": "^1.5",
        "nyholm/psr7-server": "^1",
        "pestphp/pest": "^1.21",
        "staabm/annotate-pull-request-from-checkstyle": "^1.8",
        "vimeo/psalm": "^5.7"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../conia/cms"
        },
        {
            "type": "path",
            "url": "../../conia/route"
        },
        {
            "type": "path",
            "url": "../../conia/core"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
